{
  "name": "ActiveX for Chrome",
  "version": "1.4.5",
  "description": "__MSG_plug_desp__",
  "permissions": [
    "<all_urls>",
    "tabs",
    "webRequestBlocking",
    "webRequest"
  ],
  "default_locale": "en",
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["configure.js", "inject_actions.js", "inject_start.js"],
      "run_at":"document_start"
    },
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["inject_doreplace.js"],
      "run_at":"document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "page_action": {
    "default_icon": "icon16.png"
  },
  "options_page": "options.html",
  "plugins": [{
    "path": "npactivex.dll",
    "public": true
  }]
}
